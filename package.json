{
  "name": "reactStarterKit",
  "version": "1.0.0",
  "description": "Simplest Heroku starter kit for React apps.",
  "main": "server/index.js",
  "author": "Ivan Georgiev",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --colors",
    "server": "nodemon server/index.js --exec babel-node --presets latest,react",
    "start": "babel-node server/index.js",
    "dev": "webpack-dev-server --history-api-fallback --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivangeorgiew/reactStarterKit.git"
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
